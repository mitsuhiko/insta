              "end": "(?x)\n                                (?=\n                                      \\s* $\n                                    | \\s+ \\#\n                                    | \\s* : (\\s|$)\n                                    | \\s* : [\\[\\]{},]\n                                    | \\s* [\\[\\]{},]\n                                )\n                            ",
              "name": "string.unquoted.plain.in.insta"
            }
          ]
        },
        {
          "include": "#flow-node"
        },
        {
          "begin": ":(?=\\s|$|[\\[\\]{},])",
          "captures": {
            "0": {
              "name": "punctuation.separator.key-value.mapping.insta"
            }
          },
          "end": "(?=[},\\]])",
          "name": "meta.flow-pair.insta",
          "patterns": [
            {
              "include": "#flow-value"
            }
          ]
        }
      ]
    },
    "flow-scalar": {
      "patterns": [
        {
          "include": "#flow-scalar-double-quoted"
