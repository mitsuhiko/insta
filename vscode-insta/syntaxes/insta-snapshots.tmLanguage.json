      ]
    },
    "flow-type-wrapper": {
      "begin": "\\b([\\w_]+)\\s*(\\()",
      "beginCaptures": {
        "1": {
          "name": "keyword.insta"
        },
        "2": {
          "name": "punctuation.definition.mapping.begin.insta"
        }
      },
      "end": "\\)",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.mapping.end.insta"
        }
      },
      "patterns": [
        {
          "include": "#flow-node"
        }
      ]
    },
    "flow-ron-struct": {
      "begin": "\\b([\\w_]+)\\s*(\\{)",
